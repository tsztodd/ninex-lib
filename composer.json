{
    "name": "ninex/lib",
    "description": "基础封装功能",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ninex",
            "email": "hebeshare@163.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ninex\\Lib\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ninex\\Lib\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ninex\\Lib\\LibServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
